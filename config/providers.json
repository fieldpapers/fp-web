    "options": {
      "attribution": "<a href='https://openstreetmap.org/copyright' target='_blank'>© OpenStreetMap</a>"
    },
    "minzoom": 0,
    "maxzoom": 19
  },
  "humanitarian": {
    "label": "Humanitarian",
